{
  "name": "@studio-b3/web-studio",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "web/studio",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/web/studio"
      },
      "configurations": {
        "development": {
          "outputPath": "web/studio"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "@studio-b3/web-studio:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "@studio-b3/web-studio:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "@studio-b3/web-studio:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "@studio-b3/web-studio:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "web/studio/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "web/studio/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}